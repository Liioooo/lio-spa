{
  "name": "@lio-spa/cli",
  "version": "1.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Liiioooo/lio-spa.git"
  },
  "bugs": "https://github.com/Liiioooo/lio-spa/issues",
  "author": "Liiioooo",
  "license": "MIT",
  "bin": {
    "lio-spa": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0"
  },
  "devDependencies": {
    "@lio-spa/build-app": "file:../build-app",
    "@oclif/dev-cli": "^1.22.0",
    "@types/node": "^10.14.9",
    "rimraf": "^2.6.3",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.2"
  },
  "peerDependencies": {
    "@lio-spa/build-app": "*"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/project-template",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "spa",
    "webcomponents"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "lio-spa",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "publish-package": "npm run build && npm publish",
    "build": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme"
  },
  "types": "lib/index.d.ts"
}
